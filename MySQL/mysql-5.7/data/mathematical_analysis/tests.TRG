TYPE=TRIGGERS
triggers='CREATE DEFINER=`root`@`localhost` TRIGGER deleting_test\n	BEFORE DELETE\n	ON tests\n	FOR EACH ROW\nBEGIN\nSET @DISABLE_TRIGGER = 1;\n  -- Удаляем ошибки, связанные с вопросами тестов удаляемой темы\n  DELETE osh\n    FROM mathematical_analysis.oshibki osh\n      INNER JOIN mathematical_analysis.voprosi vp\n        ON osh.id_vopr = vp.id_vopr\n      INNER JOIN mathematical_analysis.voprosi_testov vt\n        ON vp.id_vopr = vt.id_vopr\n  WHERE vt.id_tests = OLD.id_test;\n\n  -- Удаляем ошибки, связанные с попытками тестов удаляемой темы\n  DELETE osh\n    FROM mathematical_analysis.oshibki osh\n      INNER JOIN mathematical_analysis.popitki pop\n        ON osh.id_popitka = pop.id_popitka\n  WHERE pop.id_tests = OLD.id_test;\n\n  -- Далее удаляем попытки, ошибки из которых были удалены выше\n  DELETE\n    FROM mathematical_analysis.popitki\n  WHERE id_tests = OLD.id_test;\n\n  -- Удаляем записи из таблицы \'otveti\', связанные с вопросами\n  DELETE ot\n    FROM mathematical_analysis.otveti ot\n      INNER JOIN mathematical_analysis.voprosi vp\n        ON ot.id_vopr = vp.id_vopr\n      INNER JOIN mathematical_analysis.voprosi_testov vt\n        ON vp.id_vopr = vt.id_vopr\n  WHERE vt.id_tests = OLD.id_test;\n\n  -- Удаляем связи между тестами и вопросами\n  DELETE\n    FROM mathematical_analysis.voprosi_testov\n  WHERE id_tests = OLD.id_test;\n\n  -- Удаляем вопросы, связанные с тестами\n  DELETE vp\n    FROM mathematical_analysis.voprosi vp\n  WHERE vp.id_vopr IN (SELECT\n        vt.id_vopr\n      FROM mathematical_analysis.voprosi_testov vt\n      WHERE vt.id_tests = OLD.id_test);\n\n  -- Дополнительно удаляем вопросы, не связанные с тестами\n  DELETE\n    FROM mathematical_analysis.voprosi\n  WHERE id_vopr NOT IN (SELECT\n        id_vopr\n      FROM mathematical_analysis.voprosi_testov);\n        SET @DISABLE_TRIGGER = NULL;\nEND'
